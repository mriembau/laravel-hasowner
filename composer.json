{
    "name": "mriembau/laravel-hasowner",
    "description": "Add user owned behavior to eloquent models",
    "homepage": "https://github.com/mriembau/laravel-hasowner",
    "type": "package",
    "authors": [
        {
            "name": "Marc Riembau",
            "email": "mriembau89@gmail.com"
        }
    ],
    "keywords": [
        "owner",
        "owned",
        "user",
        "model",
        "laravel",
        "behaviour",
        "scope"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.5",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mriembau\\LaravelHasOwner\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mriembau\\LaravelHasOwner\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mriembau\\LaravelHasOwner\\LaravelHasOwnerServiceProvider"
            ]
        }
    }
}
